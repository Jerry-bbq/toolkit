{
  "name": "@genlib/toolkit-is",
  "version": "0.1.0",
  "description": "JavaScript 类型检测库（ESM 优先、可树摇），参考并重写了 is 库，使用 TypeScript 实现",
  "keywords": ["is", "type-check", "type-guard", "typescript", "esm"],
  "license": "MIT",
  "author": "genlib",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jerry-bbq/toolkit.git",
    "directory": "packages/is"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "lint": "biome check src"
  },
  "engines": { "node": ">=18" }
}
